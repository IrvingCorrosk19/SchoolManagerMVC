{
    "version": "3.0",
    "defaultProvider": "cdnjs",
    "libraries": [
        {
            "library": "twitter-bootstrap@5.3.3",
            "destination": "wwwroot/lib/bootstrap",
            "files": [
                "css/bootstrap.min.css",
                "js/bootstrap.bundle.min.js"
            ]
        },
        {
            "library": "admin-lte@3.2.0",
            "destination": "wwwroot/lib/adminlte",
            "files": [
                "css/adminlte.min.css",
                "js/adminlte.min.js"
            ]
        },
        {
            "library": "font-awesome@6.5.2",
            "destination": "wwwroot/lib/font-awesome",
            "files": [
                "css/all.min.css"
            ]
        },
        {
            "library": "sweetalert2@11.10.5",
            "destination": "wwwroot/lib/sweetalert2",
            "files": [
                "sweetalert2.min.js",
                "sweetalert2.min.css"
            ]
        }
    ]
}
